@article{DurrantWhyte1994,
abstract = {The mobile robot navigation problem essentially poses three questions: (1) Where am I? (2) Where am I going? (3) How do I get there? The first question is simply one of knowing where the vehicle is at all times. The second question is mainly concerned with planning and tasking. The third question is one of local and global path control. This tutorial is aimed at understanding the first of these questions; determining exactly where the robot is at all times. We will call this position determination capability localization. Localization is of paramount importance in all practical mobile robot applications. Without a localization capability it is almost impossible to address the remaining problems in navigation sensibly. In this tutorial, we will first describe the basic navigation process in terms of an algorithm called the Kalman filter. With this algorithm, we will then describe how simple beacon-based navigation systems work and show two examples of working beacon-based localization systems. We will then show how the same algorithm can be used to work out the location of a vehicle using only “natural beacons” or simply a geometric model of the robot environment, and we describe a system that does just this using an array of sonar sensors. We then move on to the somewhat harder problem of actually building and maintaining a map of the environment that can simultaneously be used in vehicle localization problems. We show that this too can be solved using similar methods to beacon-based navigation systems. Finally we outline where we believe research on navigation methods will go over the next few years, and we provide a short annotated bibliography for further reading.},
author = {Durrant‐Whyte, Hugh},
doi = {10.1108/eum0000000004145},
issn = {0143-991X},
journal = {Industrial Robot: An International Journal},
mendeley-groups = {AlpiBot},
title = {{Where am I? A tutorial on mobile vehicle localization}},
year = {1994}
}



@article{Banerjee2020,
abstract = {The recent revolutions in Internet of Things (IoT) and big data analytics have unlocked promising possibilities in biomedical and health care technologies. The current chapter deals with theoretical, methodological, validated, and empirical concepts with exciting examples related to the subject. First, the application of IoT and big data in the analysis of a vast image database generated every day from various sources by big data combined with machine-learning algorithms and other forms of artificial intelligence, to generate structured information for application in remote diagnostics, is explained with examples. Further emerging trends of telemedicine using artificial intelligence technique in robotics health care have been discussed. The flexibility and adaptability of AI-based telemedicine provide the endless supports for health care development can be observed in the literature. The advancement of telerobotic surgery and the importance of the Internet of Robotic Things (IoRT) have been discussed briefly. Furthermore, details on wearable devices already available targeting the biomedical and health care applications that are capable of collecting, analyzing by standard protocols by machine intelligence for predictions of health-related issues are described. Lastly, the final expansion and evolution of these technologies in the modern health care system and biomedical research, starting with personalized drug designing to targeted drug delivery and beyond it, is described.},
author = {Banerjee, Amit and Chakraborty, Chinmay and Kumar, Anand and Biswas, Debabrata},
doi = {10.1016/b978-0-12-818318-2.00005-2},
