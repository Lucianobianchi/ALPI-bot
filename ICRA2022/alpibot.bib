@inproceedings{Yang2018,
abstract = {Chronic obstructive pulmonary disease (COPD) is a chronic disease with long duration and repeated illness that is difficult to cure and requires long-term treatment and care. We propose a comprehensive care system that aims to provide personalized care guidance services and comprehensive, proactive and effective management of patients with different severity of COPD. The main component of the system is a patient-centered, new model of chronic disease management -Comprehensive Care Pathway. Firstly, we combine the support vector machines (SVM), the decision tree (DT) and the random forest (RF) algorithm to make a comprehensive evaluation of the patient's condition, and then provide the patient with a personalized and reasonable home rehabilitation comprehensive care plan based on the evaluation results. In addition, the system provides for the detection of events related to exacerbations of COPD and triggers an alert mechanism to alert patients and physicians that interventions can be made in time for the deterioration of patients. The system has been validated using data collected from 35 patients that have been marked by physicians and obtained satisfactory results.},
author = {Yang, Guangzhong and Kong, Chuifeng and Xu, Qinpeng},
booktitle = {Proceedings - IEEE 4th International Conference on Big Data Computing Service and Applications, BigDataService 2018},
doi = {10.1109/BigDataService.2018.00031},
